<?xml version="1.0" encoding="UTF-8"?>
<endpoint name="telflowAuthorizationTokenEndpoint" xmlns="http://ws.apache.org/ns/synapse">
    <http method="post" statistics="enable" trace="enable" uri-template="$FILE:TELFOW_AUTHORIZATION_TOKEN_EP">
        <suspendOnFailure>
            <errorCodes>-1</errorCodes>
            <initialDuration>-1</initialDuration>
            <progressionFactor>1.0</progressionFactor>
        </suspendOnFailure>
        <markForSuspension>
            <errorCodes>-1</errorCodes>
            <retriesBeforeSuspension>0</retriesBeforeSuspension>
        </markForSuspension>
    </http>
</endpoint>
